function findMax(numbers) {
  if (numbers.length === 0) {
    return null; 
  }
  
  var max = numbers[0]; 

  for (var i = 1; i < numbers.length; i++) {
    if (numbers[i] > max) {
      max = numbers[i]; 
    }
  }

  return max;
}

var numbers = [2, 8, 5, 12, 1];

var maxNumber = findMax(numbers);
console.log("The maximum number is: " + maxNumber);
